    </fragment>

    <fragment
        android:id="@+id/landingScreenFragment2"
        android:name="com.saefulrdevs.lifesync.view.started.LandingScreen2"
        android:label="StartedScreen3"
        tools:layout="@layout/fragment_landing_screen2">
        <action
            android:id="@+id/startLandingScreen3"
            app:destination="@id/landingScreenFragment3" />
    </fragment>

    <fragment
        android:id="@+id/landingScreenFragment3"
        android:name="com.saefulrdevs.lifesync.view.started.LandingScreen3"
        android:label="StartedScreen3"
        tools:layout="@layout/fragment_landing_screen3">
        <action
            android:id="@+id/startMainScreen"
            app:destination="@id/landingScreenFragment" />
    </fragment>

</navigation>