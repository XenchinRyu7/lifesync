<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_landing.xml"
    app:startDestination="@id/landingScreenFragment">
    <fragment
        android:id="@+id/landingScreenFragment"
        android:name="com.saefulrdevs.lifesync.view.started.LandingScreen"
        android:label="StartedScreen"
        tools:layout="@layout/fragment_landing_screen">
        <action
            android:id="@+id/startLandingScreen1"
            app:destination="@id/landingScreenFragment1" />
    </fragment>

    <fragment
        android:id="@+id/landingScreenFragment1"
        android:name="com.saefulrdevs.lifesync.view.started.LandingScreen1"
