<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_landing.xml"
    app:startDestination="@id/landingScreenFragment">
    <fragment
        android:id="@+id/landingScreenFragment"
        android:name="com.saefulrdevs.lifesync.ui.started.LandingApp"
        android:label="StartedScreen"
        tools:layout="@layout/fragment_landing_app">
        <action
            android:id="@+id/startLandingScreen1"
            app:destination="@id/landingScreenFragment1" />
    </fragment>

    <fragment
        android:id="@+id/landingScreenFragment1"
        android:name="com.saefulrdevs.lifesync.ui.started.TaskManagement"
        android:label="StartedScreen1"
        tools:layout="@layout/fragment_landing_task_management">
        <action
            android:id="@+id/startLandingScreen2"
            app:destination="@id/landingScreenFragment2" />
    </fragment>

    <fragment
        android:id="@+id/landingScreenFragment2"
        android:name="com.saefulrdevs.lifesync.ui.started.MoneyManagement"
        android:label="StartedScreen3"
        tools:layout="@layout/fragment_landing_money_management">
        <action
            android:id="@+id/startLandingScreen3"
            app:destination="@id/landingScreenFragment3" />
    </fragment>

    <fragment
        android:id="@+id/landingScreenFragment3"
        android:name="com.saefulrdevs.lifesync.ui.started.LandingScheduler"
        android:label="StartedScreen3"
        tools:layout="@layout/fragment_landing_scheduler">

    </fragment>

</navigation>